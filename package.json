{
  "name": "terraform-override-helper",
  "displayName": "Terraform Override Helper",
  "description": "VSCode extension to generate Terraform override files from GitLab public Terraform modules",
  "version": "0.0.2",
  "publisher": "senet",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/senet/vscode-tfoverrides.git",
    "directory": "tfoverrides"
  },
  "engines": {
    "vscode": "^1.99.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:terraformOverrideHelper.generateOverrides"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terraformOverrideHelper.generateOverrides",
        "title": "Generate Terraform Overrides from GitLab Module"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.19.86",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.99.1",
    "typescript": "^4.4.3",
    "vscode-test": "^1.6.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "vsce": "^2.15.0"
  }
}
