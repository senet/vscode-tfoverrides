{
  "name": "tfoverrides",
  "displayName": "TF Override Generator",
  "description": "A VS Code extension for generating override files from existing Terraform module repository.",
  "version": "0.1.2",
  "publisher": "senet",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/senet/vscode-tfoverrides.git",
    "directory": "tfoverrides"
  },
  "engines": {
    "vscode": "^1.99.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tfoverrides.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tfoverrides.generate",
        "title": "Generate TF Overrides"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "pretest": "npm run compile",
    "test": "mocha --require ts-node/register 'test/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.19.86",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.99.1",
    "glob": "^10.2.7",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.4.3",
    "vscode-test": "^1.6.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "vsce": "^2.15.0"
  }
}
